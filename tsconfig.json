{
  "compilerOptions": {},
  "extends": "expo/tsconfig.base",
  "moduleResolution": "node",
  "baseUrl": ".",
  "paths": {
    "@screens": ["src/screens"],
    "@screens/*": ["src/screens/*"],
    "@atoms": ["src/atoms"],
    "@atoms/*": ["src/atoms/*"],
    "@constants": ["src/constants"],
    "@constants/*": ["src/constants/*"]
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx"]
}